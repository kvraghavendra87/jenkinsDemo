pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Dev') {
            steps {
                echo 'development'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('build') {
            steps {
                echo 'building'
            }
        }
        stage('Deployement') {
            steps {
                echo 'deploying'
            }
        }
    }
}
